{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "face-recognition-backend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ],
      "links": []
    },
    {
      "name": "frontend",
      "image": "face-recognition-frontend",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["backend"]
    }
  ]
}
